  font-size: 13px;
  color: #16a8a4;
}

.text strong {
  display: inline-block;
  font-size: 16px;
  margin-bottom: 5px;
}

/* 切换标签 */

#wrap {
  width: 700px;
  margin: 10px auto;
}

#tag {
  width: 700px;
  overflow: hidden;
  background: #16a8a4;
  border: 1px solid #b9e1e0;
}

#tag li {
  list-style: none;
  float: left;
  margin-right: 0px;
  color: white;
  padding: 5px 20px;
  cursor: pointer;
}

#tag .current {
  color: #000;
  background: #ccc;
}

#tagContent div {
  /* overflow-y: scroll; */
  /* overflow-x: hidden; */
  width: 680px;
  border: 1px solid #16a8a4;
  border-top: none;
  height: 380px;
  display: none;
}

.J_tabClass {
  padding: 0px 10px;
}

