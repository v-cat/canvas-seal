.eseal-input:after {
    display: block;
    content: 'clear';
    height: 0;
    clear: both;
    overflow: hidden;
    visibility: hidden;
}

.first {
    height: 170px;
}

.fourth p,
.third p,
.seconds p,
.first p {
    padding: 0px;
    margin: 0px;
    font-weight: bold;
}

.seconds {
    height: 150px;
}

.third {
    height: 100px;
}

.fourth {
    height: 70px;
}

.seal-name {
    display: inline-block;
    padding: 0px 10px;
    color: black;
    font-size: 13px;
    width: 123px;
}

.seal-input {
    margin: 2px;
    font-size: 13px;
    height: 30px;
    line-height: 25px;
    outline: none;
    border: 0.1px solid #b9e1e0;
    padding: 0px 5px;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}

.checke {
    margin: 10px;
}

.seal-inputNum {
    width: 50px;
}

.seal-inputName {
    width: 250px;
}

.seal-all {
    width: 1200px;
    position: absolute;
}

.eseal {
    width: 250px;
    margin-left: 200px;
    float: left;
    margin-top: -150px;
}

.eseal-input {
    position: relative;
}

.eseal-name {
    font-size: 20px;
    color: #16a8a4;
    text-align: center;
}

#seal {
    width: 670px;
    top: 100px;
    margin-left: 540px;
    position: absolute;
}

