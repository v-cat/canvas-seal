.seal-bt {
    float: left;
    margin-left: 233px;
    margin-top: -35px;
    text-align: center;
}

.text {
    font-size: 12px;
    font-weight: 400;
    top: 0px;
    text-align: left;
}

.text span {
    font-size: 13px;
    color: #16a8a4;
}

.text strong {
    display: inline-block;
    font-size: 16px;
    margin-bottom: 5px;
}

/* 切换标签 */

#wrap {
    width: 700px;
    margin: 10px auto;
}

#tag {
    width: 700px;
    overflow: hidden;
    background: #16a8a4;
    border: 1px solid #b9e1e0;
}

#tag li {
    list-style: none;
    float: left;
    margin-right: 0px;
    color: white;
    padding: 5px 20px;
    cursor: pointer;
}

#tag .current {
    color: #000;
    background: #ccc;
}

#tagContent div {
    /* overflow-y: scroll; */
    /* overflow-x: hidden; */
    width: 680px;
    border: 1px solid #16a8a4;
    border-top: none;
    height: 380px;
    display: none;
}

.J_tabClass {
    padding: 0px 10px;
}

/* 公司章 */

input {
    outline: none;
}

ul {
    padding: 0px;
    margin: 0px;
    width: 480px;
    /* height: 145px; */
}

li {
    display: inline-block;
    float: left;
    list-style: none;
    margin: 0;
    padding: 3px;
}

ul strong {
    display: inline-block;
}

#canvas {
    display: none;
}

.esealSelect {
    padding: 10px;
    color: red;
    font-size: 25px;
    text-align: center;
    width: 1250px;
    margin: 0px auto;
}

.hasSlect {
    text-decoration: none;
    color: red;
    padding: 2px 10px;
    margin: 0 30px;
    border: 1px solid #16a8a4;
    /* font-size: 30px; */
}

.slected {
    background: #16a8a4;
    color: #fff;
}

.eseal-all {
    width: 1250px;
    margin: 0px auto;
    padding: 25px;
    border: 1px solid #16a8a4;
}

.image {
    margin-top: 10px;
}

.image img {
    border: 1px solid #b9e1e0;
    padding: 10px;
}

.seal-btn {
    padding: 3px 15px;
    color: #16a8a4;
    border: 1px solid #16a8a4;
}

.inputBtn:hover,
.seal-btn:hover {
    color: white;
    background: #16a8a4;
}

/* input-button */

input::-webkit-outer-spin-button,
input::-webkit-inner-spin-button {
    -webkit-appearance: none !important;
}

input[type='button'] {
    width: 20px;
    height: 17px;
    border: 1px solid #16a8a4;
    background: #ccc;
    padding: 0px 0px;
    text-decoration: none;
    cursor: pointer;
}

.inputBtn {
    position: absolute;
}

.inputBtn-top {
    display: inline-block;
    margin-top: 16px;
}

.btnSpan {
    float: right;
    width: 20px;
    height: 30px;
    margin-left: 5px;
}

.colorSpan {
    padding-right: 10px;
}

.blue {
    border-right: 20px solid blue;
}

.red {
    border-right: 20px solid red;
}

.black {
    border-right: 20px solid black;
}

